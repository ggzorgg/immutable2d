{
  "name": "immutable2d",
  "version": "0.0.2",
  "description": "Immutable 2D geometry library for Javascript/Typescript",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "jsnext:main": "lib-esm/index.js",
  "module": "lib-esm/index.js",
  "scripts": {
    "prepare": "npm run build",
    "clean": "shx rm -rf dist lib lib-esm",
    "build": "npm run clean && tsc && tsc -m es6 --outDir lib-esm && webpack",
    "test": "mocha -r ts-node/register test/**/*.spec.*",
    "testpackage": "npm pack && npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ggzorgg/immutable2d.git"
  },
  "keywords": [
    "immutable",
    "geometry",
    "typescript",
    "library",
    "utils"
  ],
  "author": "ggzorgg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ggzorgg/immutable2d/issues"
  },
  "homepage": "https://github.com/ggzorgg/immutable2d#readme",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/lodash": "^4.14.110",
    "@types/mocha": "^5.2.5",
    "chai": "^4.1.2",
    "eslint": "^5.3.0",
    "fast-check": "^1.4.0",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "shx": "^0.3.2",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  }
}
