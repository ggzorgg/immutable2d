{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "semicolon": [
            true,
            "never"
        ],
        "trailing-comma": false,
        "interface-name": false,
        "arrow-parens": false,
        "indent": [
            true,
            "spaces",
            2
        ],
        "no-unused-expression": false
    },
    "rulesDirectory": []
}